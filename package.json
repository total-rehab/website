    "change-case": "^4.1.2",
    "dotenv": "^16.0.3",
    "js-cookie": "^3.0.1",
    "next": "13.1.1",
    "normalize.css": "^8.0.1",
    "pluralize": "^8.0.0",
    "qs": "^6.11.0",
    "ra-input-rich-text": "^4.7.0",
    "react": "18.2.0",
    "react-admin": "^4.7.0",
    "react-dom": "18.2.0",
    "unfetch": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
